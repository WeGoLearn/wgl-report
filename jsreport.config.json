{
  "store": {
    "provider": "fs"
  },
  "blobStorage": {
    "provider": "fs"
  },
  "logger": {
    "console": {
      "transport": "console",
      "level": "debug"
    },
    "file": {
      "transport": "file",
      "level": "info",
      "filename": "logs/reporter.log"
    },
    "error": {
      "transport": "file",
      "level": "error",
      "filename": "logs/error.log"
    }
  },
  "allowLocalFilesAccess": true,
  "reportTimeout": 200000,
  "workers": {
    "numberOfWorkers": 2
  },
  "extensions": {
    "scripts": {
      "strategy": "http-server",
      "allowedModules": "*"
    }
  },
  "chrome": {
    "timeout": 200000,
    "launchOptions": {
      "args": ["--no-sandbox", "--disable-setuid-sandbox"]
    }
  }
}
